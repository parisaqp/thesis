@model IEnumerable<limoonad.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "UsersAdminCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("ویرایش", "UsersAdminEdit", new { id = item.Id }) |
                @Html.ActionLink("جزئیات", "UsersAdminDetails", new { id = item.Id }) |
                @Html.ActionLink("حذف", "UsersAdminDelete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
